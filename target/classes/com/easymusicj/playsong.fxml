<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/20" fx:controller="com.easymusicj.PlaySong"
            prefWidth="400" prefHeight="250">
    
    <VBox spacing="15" alignment="CENTER" layoutX="50.0" layoutY="50.0"
          AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
        
        <Label fx:id="dashboard_message" text="Welcome to EasyMusic Dashboard" />
        
        <Button fx:id="btn_playSong" text="PlaySong" onAction="#playsong" />
        
        <Button fx:id="btn_logout" text="Logout" onAction="#logout" />
        
    </VBox>
</AnchorPane>
